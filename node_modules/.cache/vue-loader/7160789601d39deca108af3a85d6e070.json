                    let dataLogIn = {\r\n                        username     : this.user.username,\r\n                        tokenAccess  : result.data.access,\r\n                        tokenRefresh : result.data.refresh\r\n                    }\r\n                    this.$emit('completedLogIn', dataLogIn)\r\n                })\r\n                .catch((error) => {\r\n                    if (error.response.status == \"401\")\r\n                        alert(\"ERROR 401: Credenciales Incorrectas.\");\r\n                });\r\n            }\r\n            \r\n        }\r\n    }\r\n</script>\r\n\r\n<style>\r\n    .loginUser{\r\n        position:absolute;\r\n        top:100px;\r\n        margin: 0;\r\n        padding: 0%;\r\n        height: 70%;\r\n        width: 100%;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n    }\r\n    .containerLoginUser {\r\n        border: 3px solid #3D550C;\r\n        border-radius: 15px;\r\n        width: 25%;\r\n        height: 60%;\r\n        display: flex;\r\n        flex-direction: column;\r\n        justify-content: center;\r\n        align-items: center;\r\n    }\r\n\r\n    .superior-panel{\r\n        display:flex;\r\n        background-color:#3D550C;\r\n        height:15%;\r\n        width:100%;\r\n        align-items: center;\r\n        align-content:center;\r\n        justify-content: center;\r\n        border-top-left-radius: 10px;\r\n        border-top-right-radius: 10px;\r\n\r\n    }\r\n    .superior-panel h2{\r\n        color: white;\r\n        font-size: 1.5rem;\r\n        align-content:center;\r\n    }\r\n\r\n    .loginUser form{\r\n        margin-top:30px;\r\n        width: 70%;\r\n        height: 100%;\r\n    }\r\n    .loginUser input{\r\n        height: 45px;\r\n        width: 100%;\r\n        box-sizing: border-box;\r\n        padding: 10px 20px;\r\n        margin-bottom: 20px;\r\n        border: 1px solid #283747;\r\n        color:black;\r\n    }\r\n    .loginUser button{\r\n        width: 100%;\r\n        height: 40px;\r\n        color: #E5E7E9;\r\n        background: #3D550C;\r\n        border: 1px solid #E5E7E9;\r\n        border-radius: 5px;\r\n        padding: 10px 25px;\r\n        margin: 5px 0;\r\n    }\r\n    .loginUser button:hover{\r\n        color: #E5E7E9;\r\n        background: crimson;\r\n        border: 1px solid #283747;\r\n    }\r\n</style>"]}]}